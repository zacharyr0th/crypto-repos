    "@tanstack/react-query-devtools": "^5.69.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bufferutil": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "critters": "^0.0.25",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.1",
    "framer-motion": "^11.18.2",
    "geist": "^1.3.1",
    "glob": "^10.3.10",
    "lodash": "^4.17.21",
    "lucide-react": "^0.483.0",
    "next": "14.1.0",
    "next-themes": "^0.4.6",
    "pg": "^8.14.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "shadcn-ui": "^0.9.5",
    "sharp": "^0.33.5",
    "sitemap": "^7.1.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^2.2.1",
