{
  "name": "crypto-repos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "reset": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "update": "ts-node scripts/update-all-data.ts",
    "update:concurrency": "ts-node scripts/update-all-data.ts --concurrency=",
    "import": "ts-node scripts/import-repos.ts",
    "import:quiet": "ts-node scripts/import-repos.ts -q",
    "import:ecosystem": "ts-node scripts/import-repos.ts --ecosystem=",
    "import:direct": "ts-node scripts/import-repos.ts --direct",
    "import:massive": "ts-node scripts/import-repos.ts --memory=16GB --batch=100",
    "import:help": "ts-node scripts/import-repos.ts --help",
    "analyze": "ANALYZE=true next build",
    "deploy": "ts-node scripts/deploy-vercel.ts",
    "hasura": "ts-node scripts/hasura.ts",
    "hasura:start": "ts-node scripts/hasura.ts start",
    "hasura:stop": "ts-node scripts/hasura.ts stop",
    "hasura:restart": "ts-node scripts/hasura.ts restart",
    "hasura:logs": "ts-node scripts/hasura.ts logs",
    "hasura:status": "ts-node scripts/hasura.ts status",
    "postbuild": "node scripts/sitemap-gen.js",
    "sitemap": "node scripts/sitemap-gen.js",
    "sitemap:check": "curl -s https://www.google.com/ping?sitemap=$(node -e \"console.log(process.env.NEXT_PUBLIC_SITE_URL || 'https://crypto-repos.com')/sitemap.xml\")",
    "test:pre-commit": "ts-node scripts/test-pre-commit.ts"
  },
  "dependencies": {
